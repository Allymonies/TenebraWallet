{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@app": ["./App.tsx"],

      "@actions": ["./store/actions"],
      "@actions/*": ["./store/actions/*"],
      "@reducers/*": ["./store/reducers/*"],
      "@store": ["./store"],
      "@store/*": ["./store/*"],

      "@comp/*": ["./components/*"],
      "@layout/*": ["./layout/*"],
      "@pages/*": ["./pages/*"],

      "@api": ["./tenebra/api"],
      "@api/*": ["./tenebra/api/*"],
      "@wallets": ["./tenebra/wallets"],
      "@wallets/*": ["./tenebra/wallets/*"],
      "@contacts": ["./tenebra/contacts"],
      "@contacts/*": ["./tenebra/contacts/*"],
      "@tenebra/*": ["./tenebra/*"],
      "@global/*": ["./global/*"],

      "@utils": ["./utils"],
      "@utils/*": ["./utils/*"]
    }
  }
}
